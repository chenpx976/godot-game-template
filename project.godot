[application]

config/name="godot-game-template"
run/main_scene="res://scenes/menu/menu.tscn"
boot_splash/image="res://assets/splash.png"
boot_splash/bg_color=Color( 0.0862745, 0.0862745, 0.0862745, 1 )
config/icon="res://assets/sprites/icon.png"
config/version="0.0.1"

[autoload]

Game="*res://addons/game-template/game.gd"
Transitions="*res://addons/game-template/transition/transition.tscn"
Utils="*res://addons/game-template/utils.gd"
LastOpenedFiles="*res://addons/file-editor/scripts/LastOpenedFiles.gd"
InputHelper="*res://addons/input_helper/input_helper.gd"
ggsManager="*res://addons/GGS/src/ggs_manager.gd"
InputBuffer="*res://singleton/input_buffer.gd"
ControllerIcons="*res://addons/controller_icons/ControllerIcons.gd"

[category]

property_name=1

[display]

window/dpi/allow_hidpi=true
window/stretch/mode="2d"
window/stretch/aspect="expand"

[editor_plugins]

enabled=PoolStringArray( "res://addons/FlexContainer/plugin.cfg", "res://addons/GGS/plugin.cfg", "res://addons/SingletonScriptsShortcut/plugin.cfg", "res://addons/controller_icons/plugin.cfg", "res://addons/file-editor/plugin.cfg", "res://addons/game-template/plugin.cfg", "res://addons/input_helper/plugin.cfg", "res://addons/monitor_overlay/plugin.cfg", "res://addons/project_map/plugin.cfg", "res://addons/rand_utils/plugin.cfg" )

[importer_defaults]

csv_translation={
"compress": true,
"delimiter": 0
}

